<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pTricKg.Sadhu"
    android:versionCode="17"
    android:versionName="1.8.0" >

    <uses-sdk android:minSdkVersion="4" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="pTricKg.Sadhu.SplashPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pTricKg.Sadhu.SadhuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="pTricKg.Sadhu.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
             </intent-filter>
          </activity>  
             <activity
            	android:name=".SadhuContent"
            	android:label="@string/app_name" >
            	<intent-filter>
                	<action android:name="pTricKg.Sadhu.CONTENT"/>
                	<category android:name="android.intent.category.DEFAULT"/>
             </intent-filter>
             </activity>    
    </application>

</manifest>